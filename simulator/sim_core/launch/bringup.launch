<launch>

  <arg name="world" default="$(env GZWORLD)"/>
  <arg name="model" default="$(env TURTLE_TYPE)"/>
  <arg name="x_pos" default="$(env TURTLE_POS_X)"/>
  <arg name="y_pos" default="$(env TURTLE_POS_Y)"/>
  <arg name="z_pos" default="$(env TURTLE_POS_Z)"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name"   value="$(arg world)"/>
    <arg name="paused"       value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui"          value="false"/>
    <arg name="headless"     value="false"/>
    <arg name="debug"        value="false"/>
  </include>

  <include file="$(find trolly)/launch/gzrobot.launch">
    <arg name="namespace" value="trolly_alphred"/>
    <arg name="x_pos"     value="0.0"/>
    <arg name="y_pos"     value="0.0"/>
    <arg name="z_pos"     value="0.0"/>
  </include>

  <!--
  <include file="$(find trolly)/launch/gzrobot.launch">
    <arg name="namespace" value="trolly_betrice"/>
    <arg name="x_pos"     value="1.0"/>
    <arg name="y_pos"     value="0.0"/>
    <arg name="z_pos"     value="0.0"/>
  </include> -->

</launch>
