# Shortcuts
function g () { echo "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" ; }
function gs () { git status ; }
function t () { clear ; tree -L 6 --noreport ~/warez/src ; }
function tc () { clear ; temp=$LS_COLORS; LS_COLORS=$(general_colours) ; tree -L 6 --noreport ~/warez/src ; export LS_COLORS=$temp ; }
function tr () { clear ; temp=$LS_COLORS; LS_COLORS=$(ros_colours) ; tree -L 6 --noreport ~/warez/src ; export LS_COLORS=$temp ; }

function w () { cd ~/warez/src/warez ; }

