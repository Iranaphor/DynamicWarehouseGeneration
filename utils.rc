alias python="python3"


function general_colours () {
  LS_COLORS=$LS_COLORS:'di=0;36:' ;
  LS_COLORS=$LS_COLORS:'*.tmule=0;31:' ;
  LS_COLORS=$LS_COLORS:'*.tmap=0;32:' ;
  LS_COLORS=$LS_COLORS:'*.net=0;33:' ;
  LS_COLORS=$LS_COLORS:'*.world=0;34:' ;
  LS_COLORS=$LS_COLORS:'*.urdf=0;35:' ;
  LS_COLORS=$LS_COLORS:'*.rc=0;37:*.txt=0;37:*.xml=0;37:*.md=0;37' ;
  echo $LS_COLORS ;
}

function ros_colours () {
  LS_COLORS=$LS_COLORS:'di=0;36:' ;
  LS_COLORS=$LS_COLORS:'*.tmule=0;31:' ;
  LS_COLORS=$LS_COLORS:'*.tmap=0;32:*.net=0;32:*.world=0;32:*.urdf=0;32' ;
  LS_COLORS=$LS_COLORS:'*.launch=0;33:' ;
  LS_COLORS=$LS_COLORS:'*.py=0;34:' ;
  LS_COLORS=$LS_COLORS:'*.rc=0;35' ;
  LS_COLORS=$LS_COLORS:'*.txt=0;37:*.xml=0;37:*.md=0;37' ;
  echo $LS_COLORS ;
}



